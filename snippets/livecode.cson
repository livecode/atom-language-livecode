    body: "$_POST_BINARY[\"${1:variable}\"]"
  "POST_RAW['…']":
    prefix: "$_"
    body: "$_POST_RAW[\"${1:variable}\"]"
  Repeat:
    prefix: "repeat"
    body: '''
      repeat ${1:loopform}
      	${0:# code...}
      end repeat
    '''
