".source.livecodescript, .source.iRev":
  "COOKIE['...']":
    prefix: "$_"
    body: "$_COOKIE[\"${1:variable}\"]"
  Command:
    prefix: "com"
    body: '''
      command ${1:name} ${2:params}
      	${3:# code...}
      end ${1}
      ${0}
    '''
  "FILES['...']":
    prefix: "$_"
    body: "$_FILES[\"${1:variable}\"]"
  Function:
    prefix: "func"
    body: '''
      function ${1:name} ${2:params}
      	${3:# code...}
      end ${1}
      ${0}
    '''
  "GET['…']":
    prefix: "$_"
    body: "$_GET[\"${1:variable}\"]"
  "GET_BINARY['…']":
    prefix: "$_"
    body: "$_GET_BINARY[\"${1:variable}\"]"
  "GET_RAW['…']":
    prefix: "$_"
    body: "$_GET_RAW[\"${1:variable}\"]"
  If:
    prefix: "if"
    body: '''
      if ${1:# code...} then
      	${2:# code...}
      end if
      ${0}
    '''
  "POST['…']":
    prefix: "$_"
    body: "$_POST[\"${1:variable}\"]"
  "POST_BINARY['…']":
