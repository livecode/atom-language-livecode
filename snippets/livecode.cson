  Switch:
    prefix: "switch"
    body: '''
      switch ${1:switchExpression}
      	case ${2:caseValue}
      		${3:# code...}
      	break
      	default
      	${4:# code...}
      end switch
      ${0}
    '''
  Try:
    prefix: "try"
    body: '''
      try
      	${1:# code...}
      catch ${2:tError}
      	${3:# code...}
      end try
      ${0}
    '''
